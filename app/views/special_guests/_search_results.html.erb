<div id="search_results">
  <table>
    <tr>
      <th>ID</th>
      <th>Forname</th>
      <th>Name</th>
      <th>UID</th>
      <th>Group</th>
      <th>Ticket Type</th>
      <th>Price</th>
    </tr>
    <% (@results || []).each do |special_guest| %>
    <tr>
      <td><%= special_guest.id %></td>
      <td><%= special_guest.forename %></td>
      <td><%= special_guest.name %></td>
      <td><%= special_guest.uid %></td>
      <td><%= special_guest.group.name %></td>
      <td><%= special_guest.ticket.name %></td>
      <td><%= nice_price(special_guest.ticket.price) %></td>
      <td>
        <%= link_to_remote(
              special_guest.ticket.name, 
              :url => add_ticket_to_cart_path(
                :id => special_guest.ticket.id,
                :reservation_id => special_guest
              ), 
              :method => :put
            ) %>
      </td>
    </tr>
    <% end %>
  </table>
</div>