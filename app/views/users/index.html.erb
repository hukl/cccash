<h1>Users</h1>

<p>Admins</p>
<table>
  <tr>
    <th>login</th>
    <th>name</th>
    <th>actions</th>
  </tr>
  <% @admins.each do |user| %>
  <tr>
    <td><%= user.login %></td>
    <td><%= user.name %></td>
    <td></td>
  </tr>
  <% end %>
</table>


